pipeline {
    agent any

    stages {
        stage('Git clone') {
            steps {
                 echo 'git clone https://github.com/akashtomar12/spring-petclinic.git'
            }
        }
        stage('Docker Build') {
            steps {
                echo 'docker build -t jenkinimage .'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
             }
            }
            stage('Test') {
            steps {
                echo 'Testing'
            }
            }
            stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
        }
        }
